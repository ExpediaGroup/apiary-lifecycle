[
    {
        "name": "${name}",
        "image": "${docker_image}:${docker_image_version}",
        ${docker_auth}
        "memory": ${memory},
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-group": "${log_group}",
                "awslogs-region": "${region}",
                "awslogs-stream-prefix": "/"
            }
        },
        "portMappings": [
            {
                "containerPort": ${port},
                "hostPort": ${port}
            }
        ],
        "environment":  [
            {
                "name": "BEEKEEPER_CONFIG",
                "value": "${beekeeper_config_yaml}"
            }
        ],
        "secrets": [
            {
                "name": "${db_password_key}",
                "valueFrom": "${db_password_arn}"
            }
        ]
    }
]
